<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- id表示获取到对象标识 class 创建哪个类的对象 -->
	<bean id="stu" class="cn.starsw.pojo.Student"><!-- 1种 -->
		<constructor-arg index="0" value="110" type="int"></constructor-arg>
		<constructor-arg index="1" value="甄" type="java.lang.String"></constructor-arg>
	<!-- 	<property name="name" value="甄兆星"></property> -->
	</bean>
	<!-- 这是实例工厂的表现形式  2
	<bean id="factory" class="cn.starsw.pojo.StudentFactory"></bean>
	<bean id="stu1" factory-bean="factory" factory-method="newInstance"></bean>
	-->
	<!-- 这是静态工厂的表现形式 静态工厂不用创建 工厂了  只需要在StudentFactory 加static  3-->  
     <bean id="stu2" class="cn.starsw.pojo.StudentFactory" factory-method="newInstance"></bean>      
</beans>